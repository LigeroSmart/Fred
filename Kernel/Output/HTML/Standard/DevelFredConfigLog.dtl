<div class="DevelFredBox">
    <h1>
        <input type="checkbox" title="Toggle this widget" checked="checked" onclick="document.getElementById('DevelFredConfigLogContent').style.display = this.checked ? 'block' : 'none';"/>
        Fred-Config-Log
    </h1>
    <div class="DevelFredBoxContent" id="DevelFredConfigLogContent">
        <table>
            <tr class="contentkey">
                <th align="right">Calls</th>
                <th>Config</th>
                <th>Status</th>
                <th>Package</th>
            </tr>
$Data{"HTMLLines"}
        </table>
    </div>
</div>
