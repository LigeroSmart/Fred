<div class="DevelFredBox">
    <h1><input type="checkbox" checked="checked" onclick="document.getElementById('FredSmallProfTable').style.display = this.checked ? 'block' : 'none';">Fred-SmallProf</h1>
    <table id="FredSmallProfTable" bgcolor="#ffffff" cellspacing="0" cellpadding="2" width="100%">
        <tr>
            <td colspan="6">
                $Text{"Profiled packages:"} $QData{"Packages"}
            </td>
        </tr>
        <tr>
            <td>
                $Text{"Calls"}
            </td>
            <td>
                $Text{"Time executing (ms)"}
            </td>
            <td>
                $Text{"Time expended (ms)"}
            </td>
            <td>
                $Text{"File"}
            </td>
            <td>
                $Text{"Line"}
            </td>
            <td>
                $Text{"Text"}
            </td>
        </tr>
$Data{"HTMLLines"}
        <tr>
            <td align="right">$Data{"TotalCall"}</td>
            <td colspan="5"> Calls</td>
        </tr>
        <tr>
            <td colspan="6">
                $Text{"For more information, especially 'time execution' and 'time expended' read perldoc SmallProf!"}
            </td>
        </tr>
        <tr>
            <td colspan="6">
                $Text{"If you change the package config you have to reload this site two times!"}
            </td>
        </tr>
    </table>
</div>
