<div class="DevelFredBox">
    <h1>
        Environment Variables
        <span class="FredToolBar">
            <i class="icon-search FredSearch" title="Search"></i>
            <i class="icon-minus FredMinimize" title="Minimize"></i>
            <i class="icon-remove FredClose" title="Close"></i>
        </span>
    </h1>
    <div class="DevelFredBoxContent">
        <div class="FredQuickSearch">
            <input type="text" placeholder="Search for..." />
            <i class="icon-remove"></i>
        </div>
        <table class="FredTableDefault">
            <thead>
                <tr>
                    <th>Key</th>
                    <th>Value</th>
                </tr>
            </thead>
[% RenderBlockStart("EnvDataRow") %]
            <tr>
                <td>[% Data.Key | html %]</td>
                <td>[% Data.Value | html %]</td>
            </tr>
[% RenderBlockEnd("EnvDataRow") %]
            <tr class="Hidden FilterMessage">
                <td colspan="2">No Items found.</td>
            </tr>
        </table>
    </div>
</div>
